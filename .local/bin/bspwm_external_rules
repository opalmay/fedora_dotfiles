#!/bin/sh

window_id="$1"
# window_class="$2"
# window_instance="$3"
# consequences="$4"
window_title="$(xwininfo -id "$window_id" | sed ' /^xwininfo/!d ; s,.*"\(.*\)".*,\1,')"

case "$window_title" in
	*"All Files â€” Portal"*)
    echo "rectangle=1300x1000+0+0"
		;;
  *"Untitled - Brave"*)
    echo "state=floating"
		;;
  *"Properties for"*)
    echo "rectangle=1000x680+0+0"
		;;
	*)
		# echo "Nothing found..."
		;;
esac
